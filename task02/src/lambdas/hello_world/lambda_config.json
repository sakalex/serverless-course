{
  "version": "1.0",
  "name": "hello_world",
  "func_name": "handler.lambda_handler",
  "resource_type": "lambda",
  "iam_role_name": "hello_world-role",
  "runtime": "python3.10",
  "memory": 128,
  "timeout": 100,
  "lambda_path": "lambdas/hello_world",
  "dependencies": [],
  "event_sources": [],
  "env_variables": {},
  "publish_version": true,
  "url_config": {
    "auth_type": "NONE",
        "cors": {
            "allow_credentials": true,
            "allow_headers": ["*"],
            "allow_methods": ["GET"],
            "allow_origins": ["*"],
            "expose_headers": ["*"],
            "max_age": 300
        },
        "source_arn": "hello_world"
  },
  "ephemeral_storage": 512,
  "logs_expiration": "${logs_expiration}"
}